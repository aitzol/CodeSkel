[buildout]
parts = 
    django
    omelette

extensions =
    buildout.eggtractor

find-links = 
    http://code.codesyntax.com/static

\# Add here any other egg you want to depend on
eggs =
   

develop = 
    \#src/my-small-egg

[django]
recipe = djangorecipe
version = ${django_version}
project = ${project}
settings = settings
eggs = 
   \${buildout:eggs}

[omelette]
recipe = collective.recipe.omelette
eggs = \${django:eggs}
packages = \${django:location}/django ./django